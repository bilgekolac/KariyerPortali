@model KariyerPortali.Admin.ViewModels.ResumeViewModel
<!-- BEGIN SAMPLE FORM PORTLET-->
<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
<link rel="stylesheet" href="~/assets/global/css/yazdir.css" media="print">

<style>
    table, td, th {
        border: none;
        text-align: left;
    }

    table {
        border-collapse: collapse;
        width: 100%;
    }

    th, td {
        padding: 15px;
    }
</style>
<br />
<div class="page-bar" id="non_write"></div>
 


<br />
<div class="page-bar" id="non_write">
    <ul class="page-breadcrumb">
        <li>
            <a href="/Resume">Özgeçmişler</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <span>Özgeçmiş Detayları</span>
        </li>
    </ul>

    <div class="page-toolbar" id="non_write">

    </div>

    <div class="btn-group pull-right">
      
        <a href="javascript:void(0)" onclick="window.print();" target="_blank" id="sample_editable_1_new" class=" btn sbold green" style="float:right">
            <i class="fa fa-print"></i> Yazdır
        </a>
    </div>
</div>
<br />


<div class="caption font dark"id="non_write">
    <div class="row">
        <div class="col-md-9">
            <div class="portlet light bordered">
                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="icon-settings font-dark"></i>
                        <span class="caption-subject bold uppercase"> ÖZGEÇMİŞ DETAYI </span>
                    </div>
                </div>
            </div>

            <div class="portlet light bordered" id="write">
                <!--ADAY BİLGİLERİ SIRALANACAK-->
                <div class="row">
                    <br />
                    <div class="col-md-4">

                        <img src="~/Uploads/Candidate/123.jpg" width="175" height="175" />

                    </div>
                    <div class="col-md-8">
                        <div class="portlet-title">
                            <div class="caption font-dark">
                                <div class="caption-subject bold uppercase">
                                    <h1 class="page-title">
                                        <strong>@Html.DisplayFor(model => model.FirstName) @Html.DisplayFor(model => model.LastName)</strong>
                                    </h1>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <br />
                <br />
                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="material-icons">local_phone</i>
                        <span class="caption-subject">İletişim Bilgileri</span>
                    </div>
                </div>



                <div class="form-body" style="font-size:inherit">
                    <div class="form-group">
                        <label><strong>E-Posta <b style="margin-left:100px">:</b> </strong>@Html.DisplayFor(model => model.Email)</label>
                    </div>
                    <div class="form-group">
                        <label><strong>Adres Bilgileri <b style="margin-left:30px">:</b> </strong>@Html.DisplayFor(model => model.Address)</label>
                    </div>
                    <div class="form-group">
                        <label><strong>Ev Telefonu <b style="margin-left:30px">:</b> </strong>@Html.DisplayFor(model => model.HomePhone)</label>
                    </div>
                    <div class="form-group">
                        <label><strong>Cep Telefonu(1) <b style="margin-left:30px">:</b> </strong>@Html.DisplayFor(model => model.CellPhone)</label>
                    </div>
                    <div class="form-group">
                        <label><strong>Cep Telefonu(2) <b style="margin-left:30px">:</b> </strong>@Html.DisplayFor(model => model.CellPhone2)</label>
                    </div>
                    <div class="form-group">
                        <label><strong>Web Adresi<b style="margin-left:30px">:</b> </strong>@Html.DisplayFor(model => model.Website) </label>
                    </div>
                </div>
                <br />



                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="material-icons">person</i>
                        <span class="caption-subject">Kişisel Bilgiler</span>
                    </div>
                </div>




                <div class="form-body">
                    <div class="form-group">


                        @if (Model.MaritalStatus == KariyerPortali.Model.MaritalStatus.Single)
                        {
                            <label><strong>Medeni Durumu <b style="margin-left:30px">:</b> </strong>Evli</label>
                        }
                        else
                        {
                            <label><strong>Medeni Durumu <b style="margin-left:30px">:</b></strong>Evli</label>
                        }

                    </div>
                    <div class="form-group">
                        <label><strong>Uyruk <b style="margin-left:30px">:</b></strong>@Html.DisplayFor(model => model.Nationality.CountryName)</label>
                    </div>

                    <div class="form-group">
                        <label><strong>TC Kimlik NO <b style="margin-left:30px">:</b></strong>@Model.CitizenshipId</label>
                    </div>
                    <div class="form-group">
                        <label><strong>Doğum Tarihi <b style="margin-left:30px">:</b></strong>@Model.BirthDate.ToShortDateString() </label>
                    </div>
                    <div class="form-group">
                        <label><strong>Doğum Yeri <b style="margin-left:30px">:</b></strong> @Html.DisplayFor(model => model.BirthCity.CityName)</label>
                    </div>
                    <div class="form-group">
                        <label><strong>Sürücü Belgesi <b style="margin-left:30px">:</b> </strong> @Html.DisplayFor(model => model.DrivingLicenseExists)</label>
                    </div>
                    <div class="form-group">
                        <label><strong>Sürücü Belgesi Türü<b style="margin-left:30px">:</b> </strong> @Html.DisplayFor(model => model.DrivingLicense1Class)</label>
                    </div>

                    <div class="form-group">

                        @if (Model.MilitaryService == KariyerPortali.Model.MilitaryService.Done)
                        {
                            <label><strong>Askerlik Durumu <b style="margin-left:30px">:</b> </strong>Yapıldı</label>
                        }
                        else if (Model.MilitaryService == KariyerPortali.Model.MilitaryService.Exempt)
                        {
                            <label><strong>Askerlik Durumu <b style="margin-left:30px">:</b></strong>Muaf</label>
                        }
                        else if (Model.MilitaryService == KariyerPortali.Model.MilitaryService.Postpone)
                        {
                            <label><strong>Askerlik Durumu <b style="margin-left:30px">:</b></strong>Tecilli</label>
                        }

                    </div>

                    <div class="form-group">
                        <label><strong>Askerlik Tecil Tarihi<b style="margin-left:30px">:</b></strong>@Model.MilitaryPostponeDate.ToShortDateString()</label>
                    </div>


                </div>

                <br />
                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="material-icons">info</i>
                        <span class="caption-subject">Özet Bilgi</span>
                    </div>
                </div>

                <div class="form-body">
                    <div class="form-group">
                        <label> @Html.DisplayFor(model => model.CoverLetter)</label>
                    </div>
                </div>

                <br />
                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="material-icons">work</i>
                        <span class="caption-subject">İş Deneyimleri</span>
                    </div>
                </div>

                <div class="form-body">
                    <div class="form-group">
                        <div class="form-group">

                            @if (Model.WorkStatus == KariyerPortali.Model.WorkStatus.Active)
                            {  <label><strong>Çalışma Durumu<b style="margin-left:30px">:</b></strong> Çalışıyor</label> }

                            else
                            {
                                <label><strong>Çalışma Durumu<b style="margin-left:30px">:</b></strong> Çalışmıyor</label>
                            }
                            <br /><br />




                        </div>

                        <table class="table" style="margin-right:auto">
                            <thead>

                            </thead>
                            <tbody>

                                @foreach (var experienceInfo in Model.Experiences)
                                {
                                    <tr>
                                        <td>
                                            <strong>@experienceInfo.Title</strong> <br /><br />@experienceInfo.CompanyName <br /><br />
                                            @experienceInfo.ExperienceStart.ToShortDateString() - @experienceInfo.ExperienceEnd.ToShortDateString() -

                                            @if (experienceInfo.ExperienceStatus == KariyerPortali.Model.ExperienceStatus.Free)
                                            { <label>Serbest</label>}
                                            else if (experienceInfo.ExperienceStatus == KariyerPortali.Model.ExperienceStatus.FullTime)
                                            { <label>Tam Zamanlı</label>}
                                            else if (experienceInfo.ExperienceStatus == KariyerPortali.Model.ExperienceStatus.Intern)
                                            { <label>Stajer</label>}
                                            else if (experienceInfo.ExperienceStatus == KariyerPortali.Model.ExperienceStatus.PartTime)
                                            { <label>Yarı Zamanlı(Part-Time)</label>}
                                            else
                                            { <label> Proje Tabanlı </label>}

                                            <br /> @experienceInfo.Description
                                        </td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>

                </div>

                <br />


                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="material-icons">school</i>
                        <span class="caption-subject">Eğitim Bilgileri</span>
                    </div>

                    <table class="table">
                        <thead>
                            <tr></tr>
                        </thead>
                        <tbody>
                            @foreach (var educationInfo in Model.EducationInfos)
                            {
                                <tr>
                                    <td>
                                        @if (educationInfo.EducationStatus == KariyerPortali.Model.EducationStatus.Doctorate)
                                        { <strong>(Doktora)</strong>}
                                        else if (educationInfo.EducationStatus == KariyerPortali.Model.EducationStatus.Master)
                                        { <strong>(Master)</strong>}
                                        else if (educationInfo.EducationStatus == KariyerPortali.Model.EducationStatus.University)
                                        { <strong>(Üniversite)</strong>}
                                        else
                                        { <strong>(M.Y.O)</strong>} <br /> @educationInfo.UniversityStart.ToShortDateString() - @educationInfo.UniversityEnd.ToShortDateString()
                                    </td>

                                    <td><strong>@educationInfo.University.UniversityName</strong> <br /> @educationInfo.Department.DepartmentName</td>

                                </tr>
                            }
                        </tbody>
                    </table>
                    <table class="table">
                        <thead>
                            <tr></tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td><strong>(Lise)</strong><br /> @Model.HighSchoolStart.ToShortDateString() - @Model.HighSchoolEnd.ToShortDateString()</td>
                                <td><strong>  @Model.HighSchoolName</strong></td>
                               
                            </tr>
                           
                        </tbody>

                    </table>
                    
                </div>
              




            <br />

            <div class="portlet-title">
                <div class="caption font-dark">
                    <i class="material-icons">public</i>
                    <span class="caption-subject">Yabancı Dil</span>
                </div>


                <table class="table">
                    <thead>
                        <tr>
                            <th></th>
                            <th>Okuma</th>
                            <th>Yazma</th>
                            <th>Konuşma</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var languageInfo in Model.LanguageInfos)
                        {
                            <tr>
                                <td><strong>@languageInfo.Language.LanguageName</strong></td>

                                <td>
                                    @if (languageInfo.ReadingLanguageLevel == KariyerPortali.Model.LanguageLevel.Advance)
                                    { <label>İleri</label>}
                                    else if (languageInfo.ReadingLanguageLevel == KariyerPortali.Model.LanguageLevel.Beginner)
                                    { <label>Başlangıç</label>}
                                    else if (languageInfo.ReadingLanguageLevel == KariyerPortali.Model.LanguageLevel.Good)
                                    {<label>İyi</label>}

                                <td>
                                    @if (languageInfo.WritingLanguageLevel == KariyerPortali.Model.LanguageLevel.Advance)
                                    { <label>İleri</label>}
                                    else if (languageInfo.WritingLanguageLevel == KariyerPortali.Model.LanguageLevel.Beginner)
                                    { <label>Başlangıç</label>}
                                    else if (languageInfo.WritingLanguageLevel == KariyerPortali.Model.LanguageLevel.Good)
                                    {<label>İyi</label>}

                                <td>
                                    @if (languageInfo.SpeakingLanguageLevel == KariyerPortali.Model.LanguageLevel.Advance)
                                    { <label>İleri</label>}
                                    else if (languageInfo.SpeakingLanguageLevel == KariyerPortali.Model.LanguageLevel.Beginner)
                                    { <label>Başlangıç</label>}
                                    else if (languageInfo.SpeakingLanguageLevel == KariyerPortali.Model.LanguageLevel.Good)
                                    {<label>İyi</label>}
                            </tr>

                        }
                    </tbody>
                </table>
            </div>

            <br />
            <div class="portlet-title">
                <div class="caption font-dark">
                    <i class="material-icons">color_lens</i>
                    <span class="caption-subject">Yetkinlikler</span>
                </div>
               <br />
                
                   
                    <table class="table">
                        <thead>
                            <tr>
                                <th>Sertifikalar</th>
                                <th></th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var certificateInfo in Model.CertificateInfos)
                            {
                                <tr>
                                    <td>@certificateInfo.FileName</td>
                                    <td> <strong>@certificateInfo.CertificateName</strong> <br />@certificateInfo.Institute - @certificateInfo.CertificateDate.ToShortDateString() </td>

                                </tr>

                            }

                        </tbody>
                    </table>                   
            


                <div class="form-body">
                    <div class="form-group">
                        <label><strong>Sınav Bilgileri</strong></label>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Sınav </th>
                                        <th>Puan</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var examInfo in Model.ExamInfos)
                                    {
                                        <tr>
                                            <td>@examInfo.Exam.ExamName</td>
                                            <td>@examInfo.Point</td>
                                        </tr>

                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <br />

                <div class="form-body">
                    <div class="form-group">
                        <label><strong>Yetenekler</strong></label>
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Yetenek </th>
                                        <th>Puan</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var skillInfo in Model.SkillInfos)
                                    {
                                        <tr>
                                            <td>@skillInfo.Skill.SkillName</td>
                                            <td>@skillInfo.Point / 5.00</td>
                                        </tr>

                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>
            </div>
            <br />
            <div class="form-group">
                <label><strong>Bilgisayar Bilgileri</strong><b style="margin-left:30px">:</b>@Html.DisplayFor(model => model.ComputerSkill)</label>
            </div>

            <br />
            <div class="portlet-title">
                <div class="caption font-dark">
                    <i class="material-icons">poll</i>
                    <span class="caption-subject">Seminerler ve Kurslar</span>
                </div>

                <table class="table" style="margin-right:auto">
                    <thead>
                        @foreach (var courseInfo in Model.Courses)
                        {

                        }
                    </thead>
                    <tbody>
                        @foreach (var courseInfo in Model.Courses)
                        {
                            <tr>
                                <td>
                                    <strong>@courseInfo.CourseName</strong> <br />@courseInfo.Institution   @courseInfo.StartDate.ToShortDateString() - @courseInfo.FinishDate.ToShortDateString() <br />@courseInfo.Time (Saat)
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>



            </div>

            <br />
            <div class="portlet-title">
                <div class="caption font-dark">
                    <i class="material-icons">library_books</i>
                    <span class="caption-subject">Burslar ve Projeler</span>
                </div>
            </div>
            <div class="form-group">
                <label><strong>Projeler<b style="margin-left:30px">:</b></strong> @Html.DisplayFor(model => model.ScholarshipAndProject)</label>
            </div>

            <br />
            <div class="portlet-title">
                <div class="caption font-dark">
                    <i class="material-icons">info_outline</i>
                    <span class="caption-subject">Ek Bilgiler</span>
                </div>
            </div>
            <div class="form-group">
                <strong>Kan Grubu<b style="margin-left:30px">:</b></strong>
                @if (Model.BloodType == KariyerPortali.Model.BloodType.ABRhN)
                { <label>AB Rh- </label> }
                else if (Model.BloodType == KariyerPortali.Model.BloodType.ABRhP)
                { <label>AB Rh+ </label> }
                else if (Model.BloodType == KariyerPortali.Model.BloodType.ARhN)
                { <label>A Rh- </label> }
                else if (Model.BloodType == KariyerPortali.Model.BloodType.ARhP)
                { <label>A Rh+ </label> }
                else if (Model.BloodType == KariyerPortali.Model.BloodType.BRhN)
                { <label>B Rh- </label> }
                else if (Model.BloodType == KariyerPortali.Model.BloodType.BRhP)
                { <label>B Rh+ </label> }
                else if (Model.BloodType == KariyerPortali.Model.BloodType.s0RhN)
                { <label>0 Rh- </label> }
                else if (Model.BloodType == KariyerPortali.Model.BloodType.s0RhP)
                { <label>0 Rh+ </label> }
            </div>
            <div class="form-group">
                <label><strong>Hobiler <b style="margin-left:30px">:</b></strong> @Html.DisplayFor(model => model.Hobby)</label>
            </div>
            <div class="form-group">
                <strong>Sigara Kullanımı <b style="margin-left:30px">:</b></strong> @if (Model.CigaretteStatus == KariyerPortali.Model.CigaretteStatus.No)
                { <label>Hayır</label>}
                else
                { <label>Evet</label>}
            </div>
            <div class="form-group">
                <strong>Beklenen Ücret<b style="margin-left:30px">:</b></strong>
                @if (Model.SalaryWaited == KariyerPortali.Model.SalaryWaited.B1000and2000)
                { <label>1000 TL - 2000 TL</label>}
                else if (Model.SalaryWaited == KariyerPortali.Model.SalaryWaited.B2000and3000)
                { <label>2000 TL - 3000 TL</label>}
                else if (Model.SalaryWaited == KariyerPortali.Model.SalaryWaited.B3000and4000)
                { <label>3000 TL - 4000 TL</label>}
                else if (Model.SalaryWaited == KariyerPortali.Model.SalaryWaited.B4000and5000)
                { <label>4000 TL - 5000 TL</label>}
                else if (Model.SalaryWaited == KariyerPortali.Model.SalaryWaited.B5000and6000)
                { <label>5000 TL - 6000 TL</label>}
                else if (Model.SalaryWaited == KariyerPortali.Model.SalaryWaited.B6000and70000)
                { <label>6000 TL - 7000 TL</label>}
                else if (Model.SalaryWaited == KariyerPortali.Model.SalaryWaited.B7000and10000)
                { <label>7000 TL - 10000 TL</label>}
                else if (Model.SalaryWaited == KariyerPortali.Model.SalaryWaited.Over10000)
                { <label>10000 TL ve Üzeri</label>}
                else
                {<label>1000 TL Altı</label> }



            </div>


            <br />
                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="material-icons">account_box</i>
                        <span class="caption-subject">Referanslar</span>
                    </div>
                    <table class="table" style="margin-right:auto">
                        <thead>

                        </thead>
                        <tbody>
                            @foreach (var referenceInfo in Model.References)
                            {
                                <tr>
                                    <td>
                                        <strong>
                                            @referenceInfo.FirstName @referenceInfo.LastName -
                                            @if (referenceInfo.ReferenceType == KariyerPortali.Model.ReferenceType.Academic)
                                            { <strong>(Akademik)</strong>}
                                            else if (referenceInfo.ReferenceType == KariyerPortali.Model.ReferenceType.BusinessFriend)
                                            { <strong>(İş Arkadaşı)</strong>}
                                            else if (referenceInfo.ReferenceType == KariyerPortali.Model.ReferenceType.BusinessManager)
                                            { <strong>(İşletmeci/Müdür)</strong>}
                                            else if (referenceInfo.ReferenceType == KariyerPortali.Model.ReferenceType.Family)
                                            { <strong>(Aile)</strong>}
                                            else if (referenceInfo.ReferenceType == KariyerPortali.Model.ReferenceType.PublicEmployee)
                                            { <strong>(Kamu Çalışanı)</strong>}
                                            else if (referenceInfo.ReferenceType == KariyerPortali.Model.ReferenceType.SocialActivity)
                                            { <strong>(Sosyal Aktivite)</strong>}
                                        </strong><br /><br />@referenceInfo.CompanyName - @referenceInfo.Sector.SectorName <br /><br /> <strong>Tel : </strong>@referenceInfo.Phone  &ensp; <strong>E-Posta : </strong> @referenceInfo.EPosta <br />
                                        <br /> @referenceInfo.Message <br /> @referenceInfo.ReferenceFile
                                    </td>

                                </tr>
                            }
                        </tbody>
                    </table>

                </div>

            </div>


        </div>

        <div class="form-actions right" >
            <button type="button"  style="margin-left:15px" class="btn blue" onclick="window.location='@Url.Action("Index","Resume")';">Özgeçmişlere Dön</button>
        </div>

        <div class="col-md-3" style="margin-top:78px">
             <div class="portlet light bordered">
                <div class="form-body">
                    <div class="form-group">
                        <label><strong>Oluşturan : </strong>@Html.DisplayFor(model => model.CreatedBy)</label>
                        <label><strong>Oluş.Tarihi : </strong>@Model.CreateDate.ToShortDateString()</label>
                        <label><strong>Değiş. Tarihi : </strong>@Model.UpdateDate.ToShortDateString()</label>
                        <label><strong>Değiştiren : </strong>@Html.DisplayFor(model => model.UpdatedBy)</label>
                    </div>
                </div>
            </div>
        </div>
    </div>



    </div>
 



