                                    <label><strong>Lise :</strong> @Html.DisplayFor(model => model.HighSchoolStart) - @Html.DisplayFor(model => model.HighSchoolEnd)</label>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="portlet-title">
                        <div class="caption font-dark">
                            <i class="material-icons">public</i>
                            <span class="caption-subject"><strong>Yabancı Dil</strong></span>
                        </div>

                    </div>
                    <div class="portlet-body form">
                        <table>
                            <tbody>
                                <tr>
                                    <th class="lg-blank">&nbsp;</th>
                                    <th>Okuma</th>
                                    <th>Yazma</th>
                                    <th>Konuşma</th>
                                </tr>

                                <tr>
                                    <td class="lang-title"><strong>  İngilizce</strong></td>
                                    <td>İleri</td>
                                    <td>İleri</td>
                                    <td>İyi</td>
                                </tr>

                                <tr>
                                <tr>
                                    <td class="lang-title"><strong>  Almanca</strong></td>
                                    <td>İleri</td>
