@model KariyerPortali.Admin.ViewModels.ResumeViewModel      
 <!-- BEGIN SAMPLE FORM PORTLET-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <style>
        table, td, th {
            border:none;
            text-align: left;
        }

        table {
            border-collapse: collapse;
            width: 100%;
        }

        th, td {
            padding: 15px;
        }
    </style>
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <a href="/Resume">Özgeçmişler</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span>Özgeçmiş Detayları</span>
            </li>
        </ul>
        <div class="page-toolbar">

        </div>
    </div>
    <br />
    <div class="portlet light bordered">
        <div class="row">
            <div class="col-md-4">
                <img src="" />
            </div>
            <div class="col-md-8">
                <h1 class="page-title"><strong>
                      @Html.DisplayFor(model => model.FirstName) @Html.DisplayFor(model => model.LastName) </strong></h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption font-dark">
                            <i class="material-icons">local_phone</i>
                            <span class="caption-subject"><strong>İletişim Bilgileri</strong></span>
                        </div>

                    </div>
                    <div class="portlet-body form">
                        <form role="form">
                            <div class="form-body">
                                <div class="form-group">
                                    <label><strong>E-Posta : </strong> @Html.DisplayFor(model => model.Email)</label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Adres Bilgileri : </strong>@Html.DisplayFor(model => model.Address)</label>
</div>
                                <div class="form-group">
                                    <label><strong>Ev Telefonu : </strong>@Html.DisplayFor(model => model.HomePhone)</label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Cep Telefonu(1) : </strong>@Html.DisplayFor(model => model.CellPhone)</label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Cep Telefonu(2) : </strong>@Html.DisplayFor(model => model.CellPhone2)</label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Web Adresi : </strong>@Html.DisplayFor(model => model.Website) </label>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="portlet-title">
                        <div class="caption font-dark">
                            <i class="material-icons">person</i>
                            <span class="caption-subject"><strong>Kişisel Bilgiler</strong></span>
                        </div>
                        
                    </div>
                    <div class="portlet-body form">
                        <form role="form">
                            <div class="form-body">
                                <div class="form-group">
                                    <label><strong>Medeni Durumu : </strong>@Html.DisplayFor(model => model.MaritalStatus) </label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Uyruk : </strong>@Html.DisplayFor(model => model.Nationality.CountryName)</label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Askerlik Durumu : </strong>@Html.DisplayFor(model => model.MilitaryService)</label>
                                </div>
                               
                                <div class="form-group">
                                    <label><strong>Doğum Tarihi : </strong>@Html.DisplayFor(model => model.BirthDate) </label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Doğum Yeri : </strong> @Html.DisplayFor(model => model.BirthCity.CityName)</label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Sürücü Belgesi : </strong> @Html.DisplayFor(model => model.DrivingLicenseExists)</label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Sürücü Belgesi Türü : </strong> @Html.DisplayFor(model => model.DrivingLicense1Class)</label>
                                </div>
                            </div>
                        </form>
                    </div>

                    <div class="portlet-title">
                        <div class="caption font-dark">
                            <i class="material-icons">info</i>
                            <span class="caption-subject"><strong>Ön Yazı</strong></span>
                           
                        </div>
                       
                   </div>
                    <div class="portlet-body form">
                        <form role="form">
                            <div class="form-body">
                                <div class="form-group">
                                    <label> @Html.DisplayFor(model => model.CoverLetter)</label>
                                </div>

                            </div>
                        </form>
                    </div>
                    <div class="portlet-title">
                        <div class="caption font-dark">
                            <i class="material-icons">work</i>
                            <span class="caption-subject"><strong>İş Deneyimleri</strong></span>
                        </div>

                    </div>
                    <div class="portlet-body form">
                        <form role="form">
                            <div class="form-body">
                                <div class="form-group">
                                    <label><strong>Toplam Tecrübe :</strong> </label>
                                </div>
                                <div class="form-group">
                                    <label><strong>Çalışma Durumu :</strong> @Html.DisplayFor(model => model.WorkStatus)</label>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="portlet-title">
                        <div class="caption font-dark">
                            <i class="material-icons">school</i>
                            <span class="caption-subject"><strong>Eğitim Bilgileri</strong></span>
                        </div>

                    </div>
                    <div class="portlet-body form">
                        <div class="form-group" style="border-bottom:1px solid #eaeaea;">
                            <label><strong>Eğitim Durumu :</strong> </label>
                        </div>
                        <form role="form">
                            <div class="form-body">
                                <div class="form-group">
                                    <label><strong>Lisans :</strong> </label><br />
                                    
                                </div>
